{
    "version": "1.0.0",
    "name": "blumilksoftware/keating",
    "type": "project",
    "description": "Keating project.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-pdo": "*",
        "fakerphp/faker": "^1.24.1",
        "guzzlehttp/guzzle": "^7.9.3",
        "inertiajs/inertia-laravel": "^1.3.3",
        "laravel/framework": "^11.45.1",
        "laravel/sanctum": "^4.1.1",
        "laravel/tinker": "^2.10.1",
        "sentry/sentry-laravel": "^4.15.0",
        "spatie/laravel-options": "^1.2.0",
        "stevebauman/purify": "^6.3.1"
    },
    "require-dev": {
        "blumilksoftware/codestyle": "^4.1.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.1",
        "phpunit/phpunit": "^11.5.23",
        "spatie/laravel-ignition": "^2.9.1"
    },
    "autoload": {
        "psr-4": {
            "Keating\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "test": [
            "@putenv XDEBUG_MODE=off",
            "@php artisan test"
        ],
        "cs": "./vendor/bin/php-cs-fixer fix --dry-run --diff --config codestyle.php",
        "csf": "./vendor/bin/php-cs-fixer fix --diff --config codestyle.php"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
